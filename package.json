{
  "name": "domainprices",
  "version": "0.0.1",
  "description": "Fast multi-currency domain price checker library across registrars.",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npx -y esbuild src/index.ts --bundle --minify --format=esm --target=es2020 --outdir=dist/ --platform=neutral",
    "test": "npm run build && node --test",
    "start": "npm run build && cp dist/index.js demo/index.js && npx serve demo",
    "generate:op-prices": "node src/scripts/generate-openprovider-prices.js",
    "generate:exchange-rates": "node src/scripts/generate-exchange-rates.js"
  },
  "homepage": "https://namewiz.github.io/domainprices",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namewiz/domainprices.git"
  },
  "bugs": {
    "url": "https://github.com/namewiz/domainprices/issues"
  },
  "author": "Justice Ogbonna <dev@justiceo.com>",
  "keywords": [
    "domain-name",
    "domain-price",
    "domain-availability",
    "price-check"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "domainprices": "./dist/cli.js"
  },
  "license": "MIT",
  "engines": {
    "node": "^22.12.0",
    "npm": "^10.9.0"
  },
  "dependencies": {},
  "devDependencies": {}
}
